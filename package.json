{
  "name": "react-keys",
  "version": "2.7.0",
  "private": false,
  "description": "Simple way to bind keyboard to react thinking with flux",
  "keywords": [
    "react",
    "keys",
    "keyboard",
    "flux"
  ],
  "main": "build",
  "author": "Canal+",
  "homepage": "https://github.com/canalplus/react-keys",
  "bugs": "https://github.com/canalplus/react-keys/issues",
  "license": "Apache-2.0",
  "repository": "https://github.com/canalplus/react-keys.git",
  "scripts": {
    "prepublish": "npm run clean && npm run build && npm run dist",
    "clean": "rimraf build && rimraf dist",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "version": "npm run build",
    "build": "BABEL_ENV=build babel src/ -d build",
    "lint": "eslint src/** test/**",
    "dist": "webpack --progress --colors --optimize-minimize",
    "test": "BABEL_ENV=test npm run lint && mocha --compilers js:babel-core/register --require test/dom-config.js test/config.js --ui bdd 'src/**/*.spec.js'",
    "coverage": "istanbul cover _mocha -- -r babel-register test/dom-config.js test/config.js src/**/*.spec.js src/**/**/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "codecov": "2.1.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "express": "4.15.2",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "10.1.0",
    "mocha": "3.3.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-addons-perf": "15.4.2",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "recompose": "0.23.1",
    "redux": "3.6.0",
    "rimraf": "2.6.1",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "sinon-test": "1.0.1",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-livereload-plugin": "0.11.0",
    "winston": "2.3.1"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-dom": "0.14.x || 15.x.x"
  }
}
