{
  "name": "react-keys",
  "version": "3.0.0-beta.11",
  "private": false,
  "description": "Simple way to bind keyboard to react thinking with flux",
  "keywords": [
    "react",
    "keys",
    "keyboard",
    "flux"
  ],
  "main": "build",
  "author": "Canal+",
  "homepage": "https://github.com/canalplus/react-keys",
  "bugs": "https://github.com/canalplus/react-keys/issues",
  "license": "Apache-2.0",
  "repository": "https://github.com/canalplus/react-keys.git",
  "scripts": {
    "prepublish": "npm run clean && npm run build && npm run dist",
    "clean": "npm i rimraf && rimraf build && rimraf dist",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "version": "npm run build",
    "build": "BABEL_ENV=build babel src/ -d build",
    "lint": "eslint src/** test/**",
    "dist": "webpack --progress --colors --optimize-minimize",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register --require test/dom-config.js test/config.js --ui bdd 'src/**/*.spec.js'",
    "coverage": "istanbul cover _mocha -- -r babel-register test/dom-config.js test/config.js src/**/*.spec.js src/**/**/*.spec.js",
    "deploy-local": "sed -i.bak s/$npm_package_version/$npm_package_version-SNAPSHOT/g package.json && rm package.json.bak && npm pack && git checkout package.json dist/reactKeys.js && echo \"\n\n\nNow run:\n\nnpm install $(pwd)/react-keys-$npm_package_version-SNAPSHOT.tgz\n\nIf you want to install previous version just run 'npm install'.\""
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chai": "4.1.0",
    "chai-enzyme": "0.8.0",
    "codecov": "2.2.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "express": "4.15.3",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "11.1.0",
    "mocha": "3.4.2",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-addons-perf": "15.4.2",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-test-renderer": "15.6.1",
    "recompose": "0.24.0",
    "redux": "3.7.2",
    "rimraf": "2.6.1",
    "sinon": "2.4.0",
    "sinon-chai": "2.12.0",
    "sinon-test": "2.0.0",
    "webpack": "3.4.1",
    "webpack-dev-middleware": "1.11.0",
    "webpack-livereload-plugin": "0.11.0",
    "winston": "2.3.1"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-dom": "0.14.x || 15.x.x"
  },
  "dependencies": {
    "npm": "^5.3.0"
  }
}
