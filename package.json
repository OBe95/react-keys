{
  "name": "react-keys",
  "version": "2.0.0-rc18",
  "private": false,
  "description": "Simple way to bind keyboard to react thinking with flux",
  "keywords": [
    "react",
    "keys",
    "keyboard",
    "flux"
  ],
  "main": "build",
  "author": "Canal+",
  "homepage": "https://github.com/canalplus/react-keys",
  "bugs": "https://github.com/canalplus/react-keys/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/canalplus/react-keys.git"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build && npm run dist",
    "clean": "rimraf build && rimraf dist",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "version": "npm run build",
    "build": "BABEL_ENV=build babel src/ -d build",
    "lint": "eslint src/** test/**",
    "dist": "webpack --progress --colors --optimize-minimize",
    "test": "BABEL_ENV=test npm run lint && mocha --compilers js:babel-core/register --require test/dom-config.js test/config.js --ui bdd src/**/*.spec.js src/**/**/*.spec.js",
    "coverage": "istanbul cover _mocha -- -r babel-register test/dom-config.js test/config.js src/**/*.spec.js src/**/**/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "codecov": "1.0.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "react": "15.4.1",
    "react-addons-perf": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "5.0.1",
    "recompose": "0.21.1",
    "redux": "3.6.0",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-livereload-plugin": "0.9.0",
    "winston": "2.3.0"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-dom": "0.14.x || 15.x.x"
  }
}
